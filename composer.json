{
    "name": "yejune/php-ssr-react",
    "description": "PHP Server-Side Rendering for React using QuickJS FFI",
    "type": "library",
    "license": "MIT",
    "keywords": ["php", "ssr", "react", "quickjs", "ffi", "server-side-rendering"],
    "authors": [
        {
            "name": "Yejune",
            "email": "max@blue.tools"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ffi": "*"
    },
    "autoload": {
        "psr-4": {
            "PhpSsrReact\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": "scripts/download-quickjs.sh",
        "post-update-cmd": "scripts/download-quickjs.sh",
        "build-quickjs": "scripts/build-quickjs.sh"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
